<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="Suite2" verbose="1" >
    <test name="Parser tests valid xml" >
        <groups>
            <run>
                <exclude name="invalid xml"  />
                <include name="valid xml"  />
            </run>
        </groups>
        <packages>
            <package name="parsertests" />
        </packages>
    </test>
</suite>
